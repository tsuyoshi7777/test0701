{
    "executionRoleArn": "arn:aws:iam::823104559115:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "sample-website",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        },
        {
            "name": "sample-website2",
            "image": "<IMAGE2_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8001,
                    "protocol": "tcp",
                    "containerPort": 8001
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}
